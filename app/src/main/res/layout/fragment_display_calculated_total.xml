<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DisplayCalculatedTotal">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/lblSummary"
            style="@style/headingFontText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/CalcSummary"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.145" />

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView"
            android:layout_width="327dp"
            android:layout_height="385dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.546">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/ThemeCol4">

                <TextView
                    android:id="@+id/caloriesLbl"
                    style="@style/generalFontText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/CalorieLbl"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.791"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.044" />

                <TextView
                    android:id="@+id/textView26"
                    android:layout_width="107dp"
                    android:layout_height="33dp"
                    android:layout_marginTop="20dp"
                    android:hint="Answer"
                    android:textAlignment="center"
                    app:layout_constraintBottom_toTopOf="@+id/lbl5_disUp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/carbLbl"
                    app:layout_constraintTop_toBottomOf="@+id/caloriesLbl"
                    app:layout_constraintVertical_bias="0.339" />

                <TextView
                    android:id="@+id/heading_medSchedules"
                    android:layout_width="107dp"
                    android:layout_height="33dp"
                    android:layout_marginBottom="44dp"
                    android:hint="Answer"
                    android:textAlignment="center"
                    app:layout_constraintBottom_toTopOf="@+id/textView33"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/fatLbl" />

                <TextView
                    android:id="@+id/textView33"
                    android:layout_width="107dp"
                    android:layout_height="33dp"
                    android:hint="Answer"
                    android:textAlignment="center"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.845"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.815" />

                <TextView
                    android:id="@+id/lbl5_disUp"
                    android:layout_width="107dp"
                    android:layout_height="33dp"
                    android:layout_marginBottom="40dp"
                    android:hint="Answer"
                    android:textAlignment="center"
                    app:layout_constraintBottom_toTopOf="@+id/heading_medSchedules"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/protLbl" />

                <TextView
                    android:id="@+id/totCaloriesLbl"
                    style="@style/generalFontText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/TotalCalLbl"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.121"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.811" />

                <TextView
                    android:id="@+id/carbLbl"
                    style="@style/generalFontText"
                    android:layout_width="50pt"
                    android:layout_height="wrap_content"
                    android:text="@string/CalorieRecLbl1"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/textView26"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.209" />

                <TextView
                    android:id="@+id/fatLbl"
                    style="@style/generalFontText"
                    android:layout_width="50pt"
                    android:layout_height="wrap_content"
                    android:text="@string/CalorieRecLbl3"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/heading_medSchedules"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.606" />

                <TextView
                    android:id="@+id/protLbl"
                    style="@style/generalFontText"
                    android:layout_width="50pt"
                    android:layout_height="wrap_content"
                    android:text="@string/CalorieRecLbl2"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/lbl5_disUp"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.396" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <Button
            android:id="@+id/backbtn_CalCalories"
            style="@style/buttonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/GreenBtn"
            android:text="@string/BackBtnLbl"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.903" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>