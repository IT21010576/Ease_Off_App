<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/IndividualDashboardCol1"
    tools:context=".MyProfile">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:layout_width="322dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="100dp"
        android:text="@string/Myprobtn"
        android:textColor="@color/black"
        android:textSize="60sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/SignUpNameLbl"
        android:textSize="20sp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="120dp"/>

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/Fname"
        android:inputType="textPersonName"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="110dp"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Age:"
        android:textSize="20sp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="170dp"/>

    <EditText
        android:id="@+id/editTextAge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/p_age"
        android:inputType="number"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="160dp"/>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Height:"
        android:textSize="20sp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="230dp"/>

    <EditText
        android:id="@+id/editTextHeight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/p_Height"
        android:inputType="number"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="220dp"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Weight"
        android:textSize="20sp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="280dp"/>

    <EditText
        android:id="@+id/editTextweight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/p_weight"
        android:inputType="number"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="270dp"/>

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        android:textSize="20sp"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="350dp"
        android:backgroundTint="@color/ThemeCol1Btn"/>


</FrameLayout>